import static org.junit.Assert.*;

import org.junit.Test;

public class PhDTest {

	//test case input args
	private String n1="Bob";
	private String n2="Jess";
	private int y1=1995;
	private int y2=1996;
	private int m1=5;
	private int m2=6;
	private char g1='m';
	private char g2='f';
	
	//test1 for group A
	public void testConstructor1() {
		PhD ts1= new PhD(n1,y1,m1,g1);
		assert ts1.getName().equals(n1);
		assert ts1.getYear() == y1;
		assert ts1.getMonth() == m1;
		assert !(ts1.isFemale());
		assert ts1.advisor1()==null;
		assert ts1.advisor2()==null;
		assert ts1.numAdvisees()==0;
		System.out.println("Test subject 1 passes");
		
		PhD ts2= new PhD(n2,y2,m2,g2);
		assert ts2.getName().equals(n2);
		assert ts2.getYear() == y2;
		assert ts2.getMonth() == m2;
		assert ts2.isFemale();
		assert ts2.advisor1()==null;
		assert ts2.advisor2()==null;
		assert ts2.numAdvisees()==0;
		System.out.println("Test subject 2 passes");
	}

	public void testGroupB() {
		PhD ts1= new PhD(n1,y1,m1,g1);
		PhD ts2= new PhD(n2,y2,m2,g2);
	}
	
	public static void main(String[] args) {
		

	}
	
}
